{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "string",
      "required": true
    },
    "Patients": {
      "type": "string"
    },
    "Year_of_Experience": {
      "type": "string",
      "required": true
    },
    "StartTime": {
      "type": "time",
      "required": true
    },
    "EndTime": {
      "type": "time",
      "required": true
    },
    "About": {
      "type": "richtext",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "doctors"
    },
    "Phone": {
      "type": "string",
      "required": true
    },
    "Image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "Premium": {
      "type": "boolean",
      "required": true
    },
    "appointments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctor"
    }
  }
}
